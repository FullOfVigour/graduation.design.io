{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    [
      "component",
      [
        {
          "libraryName": "element-ui",
          "styleLibraryName": "theme-chalk"
        }
      ]
    ],
    ["transform-vue-jsx"]
  ],
  "comments": false,
  "env": {
    "jyjh": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
